{
    "manifest_version": 2,
    "name": "sample",
    "description": "A sample extension to get the selected text: https://stackoverflow.com/a/34181338/399696",
    "version": "1.0",
    "icons": {
        "48": "online.png"
    },
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "dashboard.html",
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ]
}
